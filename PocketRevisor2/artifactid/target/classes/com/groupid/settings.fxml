<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" stylesheets="@stylesheets/MainStyler.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.groupid.SettingsController">
    <children>
        <Label text="All entries">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <Button fx:id="secondaryButton1" onAction="#deleteWorkingDay" text="Get old entries" />
      <ScrollPane fx:id="scrollPane" prefHeight="200.0" prefWidth="200.0">
         <content>
            <VBox fx:id="root" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0" />
         </content>
      </ScrollPane>
        <Button fx:id="secondaryButton" onAction="#switchToNewEntry" text="New entry" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
